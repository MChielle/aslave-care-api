{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore.Mvc.Razor.Internal": "Warning",
        "Microsoft.AspNetCore.Mvc.Razor.Razor": "Debug",
        "Microsoft.AspNetCore.Mvc.Razor": "Error"
      }
    },
    "GELF": {
      "LogLevel": {
        "Default": "Information",
        "Gelf.Extensions.Logging.Samples.AspNetCore2.Controllers": "Debug"
      }
    }
  },
  "AllowedHosts": "*",
  "TokenConfigurations": {
    "Audience": "aslavecare-user",
    "Issuer": "aslavecare-api",
    "RefreshTokenValidity": 600,
    "AccessTokenValidity": 300
  }
}